# RevOps Hub - Environment Variables
# Copy this file to .env.local and fill in your values

# ===========================================
# SERVER-SIDE ONLY (secure, never exposed to browser)
# ===========================================

# Foundry API endpoint (e.g., https://your-stack.palantirfoundry.com)
FOUNDRY_URL=

# Foundry API token (keep secret!)
FOUNDRY_TOKEN=

# ===========================================
# CLIENT-SIDE (can be exposed to browser)
# ===========================================

# Set to 'true' to use mock data instead of Foundry
NEXT_PUBLIC_USE_MOCK_DATA=true
